<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/app_dialog_notice_note_title"
        style="@style/UIKit.Font.Big.Dark"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp" />

    <LinearLayout
        android:id="@+id/app_dialog_notice_note_item_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:orientation="vertical" />

    <View
        android:id="@+id/app_dialog_notice_note_diving"
        style="@style/UIKit.Line.Horizontal" />

    <TextView
        android:id="@+id/app_dialog_notice_note_desc"
        style="@style/UIKit.Font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:visibility="gone"/>
</LinearLayout>